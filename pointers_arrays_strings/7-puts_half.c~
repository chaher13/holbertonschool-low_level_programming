#include "main.h"

/**
 * puts_half - prints half of a string
 * @str: the string to print
 * Return: void
 */

void puts_half(char *str)
{
int n = 0;
int k;

while (str[j] != '\0')
{
n++;
}
if (n % 2 == 1)
{
k = (n - 1) / 2;
k += 1;
}
else
{
k = n / 2;
}
for (; k < n; k++)
{
_putchar(str[k]);
}
_putchar('\n');
}
